#! /usr/remote/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x18c83f0 .scope module, "TestDatamem" "TestDatamem" 2 3;
 .timescale 0 0;
v0x18f0260_0 .var "Addr", 31 0;
v0x18f0340_0 .var "MemRead", 0 0;
v0x18f0410_0 .var "MemWrite", 0 0;
v0x18f0510_0 .net "Rdata", 31 0, v0x18efdd0_0;  1 drivers
v0x18f05e0_0 .var "Wdata", 31 0;
v0x18f06d0_0 .var "clk", 0 0;
S_0x18d6670 .scope module, "DMem" "datamem" 2 13, 3 4 0, S_0x18c83f0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "MemWrite"
    .port_info 2 /INPUT 1 "MemRead"
    .port_info 3 /INPUT 32 "Addr"
    .port_info 4 /INPUT 32 "Wdata"
    .port_info 5 /OUTPUT 32 "Rdata"
v0x18d68e0_0 .net "Addr", 31 0, v0x18f0260_0;  1 drivers
v0x18efc40_0 .net "MemRead", 0 0, v0x18f0340_0;  1 drivers
v0x18efd00_0 .net "MemWrite", 0 0, v0x18f0410_0;  1 drivers
v0x18efdd0_0 .var "Rdata", 31 0;
v0x18efeb0_0 .net "Wdata", 31 0, v0x18f05e0_0;  1 drivers
v0x18effe0_0 .net "clk", 0 0, v0x18f06d0_0;  1 drivers
v0x18f00a0 .array "mem", 1048581 1048576, 31 0;
E_0x18dc2a0 .event posedge, v0x18effe0_0;
    .scope S_0x18d6670;
T_0 ;
    %vpi_call 3 16 "$readmemh", "inputmem.hex", v0x18f00a0 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x18d6670;
T_1 ;
    %wait E_0x18dc2a0;
    %load/vec4 v0x18efd00_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x18efeb0_0;
    %load/vec4 v0x18d68e0_0;
    %subi 1048576, 0, 32;
    %ix/vec4 4;
    %store/vec4a v0x18f00a0, 4, 0;
    %vpi_call 3 27 "$writememh", "inputmem.hex", v0x18f00a0 {0 0 0};
T_1.0 ;
    %load/vec4 v0x18efc40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %load/vec4 v0x18d68e0_0;
    %subi 1048576, 0, 32;
    %ix/vec4 4;
    %load/vec4a v0x18f00a0, 4;
    %store/vec4 v0x18efdd0_0, 0, 32;
T_1.2 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x18c83f0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x18f06d0_0, 0, 1;
    %pushi/vec4 1048577, 0, 32;
    %store/vec4 v0x18f0260_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x18f0410_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x18f0340_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x18f05e0_0, 0, 32;
    %delay 50, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x18f06d0_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x18f06d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x18f0340_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x18f06d0_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x18f06d0_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x18f0340_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x18f0410_0, 0, 1;
    %pushi/vec4 15, 0, 32;
    %store/vec4 v0x18f05e0_0, 0, 32;
    %delay 50, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x18f06d0_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x18f06d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x18f0340_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x18f0410_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x18f06d0_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x18f06d0_0, 0, 1;
    %delay 1000, 0;
    %vpi_call 2 53 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x18c83f0;
T_3 ;
    %vpi_call 2 59 "$monitor", $time, "in %m, Rdata = %08x, Wdata = %08x", v0x18f0510_0, v0x18f05e0_0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x18c83f0;
T_4 ;
    %delay 50, 0;
    %load/vec4 v0x18f06d0_0;
    %inv;
    %store/vec4 v0x18f06d0_0, 0, 1;
    %jmp T_4;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
